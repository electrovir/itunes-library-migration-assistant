{
    "name": "itunes-library-migration-assistant",
    "version": "0.0.1",
    "description": "",
    "keywords": [
        "vir"
    ],
    "homepage": "https://github.com/electrovir/itunes-library-migration-assistant#readme",
    "bugs": {
        "url": "https://github.com/electrovir/itunes-library-migration-assistant/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/electrovir/itunes-library-migration-assistant.git"
    },
    "license": "MIT",
    "author": "electrovir (github.com/electrovir)",
    "main": "dist/index.js",
    "bin": "dist/api/bin.js",
    "scripts": {
        "prepublishOnly": "rm -rf dist && npm run test && npm run spellcheck",
        "test:full": "npm run test && npm run spellcheck && npm run format:check",
        "test": "tsc && test-vir \"./dist/**/!(*.type|*.speed|*.sample).test.js\"",
        "test:speed": "rm -rf dist && tsc && test-vir \"./dist/**/*.speed.test.js\"",
        "test:samples": "rm -rf dist && tsc && test-vir \"./dist/**/*.sample.test.js\"",
        "spellcheck": "cspell --no-progress --unique \"./**/*\"",
        "format": "prettier --ignore-path .gitignore --write  \"./**/*.{ts,css,html,json}\"",
        "format:check": "prettier --ignore-path .gitignore --check  \"./**/*.{ts,css,html,json}\""
    },
    "dependencies": {
        "byline": "^5.0.0",
        "fsm-vir": "^0.0.5",
        "libxmljs": "^0.19.7",
        "txml": "^4.0.1",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@types/byline": "^4.2.32",
        "@types/libxmljs": "^0.18.6",
        "@types/node": "^14.14.41",
        "@types/sax": "^1.2.1",
        "@types/xml2js": "^0.4.8",
        "cspell": "^5.3.12",
        "fast-xml-parser": "^3.19.0",
        "htmlparser2": "^6.1.0",
        "prettier": "^2.2.1",
        "prettier-plugin-jsdoc": "^0.3.20",
        "prettier-plugin-organize-imports": "^1.1.1",
        "prettier-plugin-packagejson": "^2.2.10",
        "prettier-plugin-sort-json": "^0.0.2",
        "sax": "^1.2.4",
        "test-vir": "^0.0.12",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4",
        "xml-js": "^1.6.11"
    }
}
